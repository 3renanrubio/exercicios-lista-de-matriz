#include <stdio.h>

int main() {
    int matriz[3][3];
    int i, j;

    printf("Digite os valores para preencher a matriz:\n");
    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++) {
            printf("digite os valores da posição [%d][%d]: ", i, j);
            scanf("%d", &matriz[i][j]);
        }
    }

    printf("\n O valor é:\n");
    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++) {
            printf("%d", matriz[i][j]);
        }
        
        printf("\n");
    }

    return 0;
}
